    {
      "key": "confluence-content-properties-addon",
      "name": "Content Properties",
      "description": "Display content properties inside a 'Page Properties' macro",
      "vendor": {
        "name": "Don Page",
        "url": "https://www.pagedon.com"
      },
      "baseUrl": "{{localBaseUrl}}",
      "links": {
        "self": "{{localBaseUrl}}/atlassian-connect.json",
        "homepage": "{{localBaseUrl}}/atlassian-connect.json"
      },
      "authentication": {
        "type": "jwt"
      },
      "lifecycle": {
        "installed": "/installed"
      },
      "scopes": [
        "READ"
      ],
      "modules": {
        "dynamicContentMacros": [
          {
            "key": "content-properties-macro",
            "name": {
              "value": "Content Properties Macro"
            },
            "url": "/macro",
            "description": {
              "value": "Display a table of the content properties inside the 'Page properties' macro"
            },
            "outputType": "block",
            "bodyType": "none"
          }
        ]
      }
    }







